{
    "compileOnSave": false,
    "compilerOptions": {
        "noEmit": false,
        "noEmitOnError": false,
        "sourceMap": true,
        "module": "es2015",
        "target": "es5",
        "moduleResolution": "node",
        "outDir": "./dist/out-tsc",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strict": true,
        "strictPropertyInitialization": false,
        "noImplicitThis": true,
        "suppressImplicitAnyIndexErrors": true,
        "declaration": false,
        "strictNullChecks": true,
        "importHelpers": true,
        "skipLibCheck": true,
        "noEmitHelpers": true,
        "baseUrl": ".",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2017",
            "dom"
        ]
    }
}
